{
  "variables": [
    {
      "name": "pluginname",
      "question": "Please specify a Pluginname",
      "default": "Custom Post"
    }
  ],
  "replacements": [
    {
      "search": "pluginname",
      "replace": "{{pluginname}}",
      "path": "custom-posts.php"
    },
    {
      "search": "pluginname",
      "replace": "{{pluginname}}",
      "path": "single-custom-posts.php"
    },
    {
      "search": "single-custom-posts.php",
      "replace": "single-{{pluginname|lower}}.php"
    }      
  ] 
}
